{
  "metrics": {
    "namespace": "Custom/OnPremApp",
    "metrics_collected": {
      "memory": {
        "measurements": ["mem_used_percent"],
        "collection_interval": 60
      },
      "disk": {
        "measurements": ["disk_used_percent"],
        "resources": ["*"],
        "collection_interval": 60
      }
    }
  }
}
